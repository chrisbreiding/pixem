header {
  align-items: center;
  background: #ece252;
  display: flex;
  justify-content: center;
  padding: 0.4em;
}

h1 {
  font-size: 3em;
  position: relative;
  top: 0.1em;
}

.icon {
  float: left;
  font-size: 4em;
  margin-right: 0.4em;
}

.images {
  display: flex;
  flex-wrap: wrap;

  .image {
    background: #333;
    background-position: 50% 50%;
    background-repeat: no-repeat;
    background-size: contain;
    margin: 5px;
    height: 150px;
    width: 100px;
  }
}
